@model ActivityViewModel

@{
    ViewData["Title"] = "Delete";
}

<div class="container-with-padding">
    <h2>Are you sure you want to delete this activity?</h2>
    <label class="control-label" asp-for="Type" ></label>
    <label class="control-label" asp-for="Date" ></label>
    <label class="control-label" asp-for="Description" ></label>
    <div class="form-group">
        <input type="submit" class="btn btn-danger" value="Delete" />
        <input type="button" class="btn btn-default" onclick="history.back();" value="Back" />
    </div>
</div>
